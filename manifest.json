{
  "name": "console.diff() Devtool",
  "description": "Compare complex in-memory objects with console.diff command. For example: console.diff({a:1,b:1}, {a:1,b:2,c:3});",
  "version": "1.1",
  "manifest_version": 2,
  "minimum_chrome_version": "64.0",
  "devtools_page": "src/jsdiff-devtools.html",
  "icons": {
    "128": "src/img/panel-icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "src/js/jsdiff-background.js"
    ]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "file:///*",
    "clipboardWrite"
  ]
}
